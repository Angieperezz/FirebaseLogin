<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <size
                android:height="24dp"
                android:width="24dp" />
            <stroke
                android:color="@color/colorPrimaryButton"
                android:width="0.1dp" />
            <solid
                android:color="#FFFFFF" />
        </shape>
    </item>
    <item
        android:left="10dp"
        android:top="10dp"
        android:right="10dp"
        android:bottom="10dp">
        <shape android:shape="oval">
            <size
                android:height="8dp"
                android:width="8dp" />
            <solid
                android:color="@color/colorPrimaryButton" />
        </shape>
    </item>
</layer-list>